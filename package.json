{
  "name": "nanites",
  "version": "0.0.1",
  "description": "My pet bot to manage my open source projects",
  "author": "Ashwin Hegde <ashwin.hegde3@gmail.com>",
  "license": "MIT",
  "repository": "https://github.com/my-thought-experiments/nanites.git",
  "homepage": "https://github.com/my-thought-experiments/nanites",
  "bugs": "https://github.com/my-thought-experiments/nanites/issues",
  "keywords": [
    "probot",
    "github",
    "probot-app"
  ],
  "scripts": {
    "dev": "nodemon",
    "start": "probot run ./src/index.js",
    "start:prod": "probot run ./dist/index.js",
    "lint": "standard --fix",
    "test": "jest && standard",
    "test:watch": "jest --watch --notify --notifyMode=change --coverage",
    "build": "babel ./src --out-dir ./dist"
  },
  "dependencies": {
    "probot": "^7.2.0"
  },
  "devDependencies": {
    "@babel/cli": "^7.11.5",
    "@babel/core": "^7.11.5",
    "@babel/plugin-transform-regenerator": "^7.10.4",
    "@babel/preset-env": "^7.11.5",
    "jest": "^22.4.3",
    "nock": "^13.0.4",
    "nodemon": "^1.17.2",
    "smee-client": "^1.0.2",
    "standard": "^10.0.3"
  },
  "engines": {
    "node": ">= 8.3.0"
  },
  "standard": {
    "env": [
      "jest"
    ]
  },
  "jest": {
    "testEnvironment": "node"
  },
  "nodemonConfig": {
    "exec": "npm start",
    "watch": [
      ".env",
      "."
    ]
  }
}
